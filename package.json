{
  "name": "krgeobuk-shared-lib",
  "private": true,
  "version": "1.0.0",
  "description": "krgeobuk 관련 서비스에서 사용할 공통 패키지",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "npm:i:core": "pnpm --filter @krgeobuk/core install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose -f docker-compose.yaml down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-orgin": "eslint src/**/*.{ts,js} --format=stylish",
    "lint-fix-orgin": "eslint src/**/*.{ts,js} --fix",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^20.19.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
