{
  "name": "@krgeobuk/database-config",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf dist && rm -rf node_modules",
    "verdaccio:login": "pnpm login --registry=http://localhost:4873",
    "verdaccio:publish": "pnpm publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./constants": "./dist/constants/index.js",
    "./redis": "./dist/redis/index.js",
    "./typeorm": "./dist/typeorm/index.js"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "constants": [
        "dist/constants/index.d.ts"
      ],
      "constants/*": [
        "dist/constants/*"
      ],
      "redis": [
        "dist/redis/index.d.ts"
      ],
      "redis/*": [
        "dist/redis/*"
      ],
      "typeorm": [
        "dist/typeorm/index.d.ts"
      ],
      "typeorm/*": [
        "dist/typeorm/*"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "ioredis": "^5.5.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/typeorm": "^10.0.0",
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
