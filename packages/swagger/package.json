{
  "name": "@krgeobuk/swagger",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf dist && rm -rf node_modules",
    "verdaccio:login": "pnpm login --registry=http://localhost:4873",
    "verdaccio:publish": "pnpm publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config/index.js",
    "./decorators": "./dist/decorators/index.js"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "config": [
        "dist/config/index.d.ts"
      ],
      "config/*": [
        "dist/config/*"
      ],
      "decorators": [
        "dist/decorators/index.d.ts"
      ],
      "decorators/*": [
        "dist/decorators/*"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "dependencies": {
    "@krgeobuk/core": "workspace:*",
    "@nestjs/config": "^4.0.2",
    "class-validator": "^0.14.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/swagger": "^7.0.0",
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
