{
  "name": "@krgeobuk/swagger",
  "version": "1.0.0",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dtos": "./dist/dtos/index.js",
    "./decorators": "./dist/decorators/index.js",
    "./types": "./dist/types/index.js"
  },
  "typesVersions": {
    "*": {
      "dtos": [
        "dist/dtos/index.d.ts"
      ],
      "dtos/*": [
        "dist/dtos/*"
      ],
      "decorators": [
        "dist/decorators/index.d.ts"
      ],
      "decorators/*": [
        "dist/decorators/*"
      ],
      "types": [
        "dist/types/index.d.ts"
      ],
      "types/*": [
        "dist/types/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean"
  },
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "dependencies": {
    "@krgeobuk/core": "workspace:*",
    "class-validator": "^0.14.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3"
  },
  "devDependencies": {
    "@nestjs/swagger": "^11.2.0",
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}

