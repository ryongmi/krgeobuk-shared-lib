{
  "name": "@krgeobuk/account-merge",
  "version": "1.0.0",
  "description": "Account merge workflow interfaces and patterns for krgeobuk ecosystem",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf dist && rm -rf node_modules",
    "verdaccio:login": "pnpm login --registry=http://localhost:4873",
    "verdaccio:publish": "pnpm publish"
  },
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./codes": {
      "import": "./dist/codes/index.js",
      "types": "./dist/codes/index.d.ts"
    },
    "./dtos": {
      "import": "./dist/dtos/index.js",
      "types": "./dist/dtos/index.d.ts"
    },
    "./exception": {
      "import": "./dist/exception/index.js",
      "types": "./dist/exception/index.d.ts"
    },
    "./messages": {
      "import": "./dist/messages/index.js",
      "types": "./dist/messages/index.d.ts"
    },
    "./tcp": {
      "import": "./dist/tcp/index.js",
      "types": "./dist/tcp/index.d.ts"
    },
    "./tcp/patterns": {
      "import": "./dist/tcp/patterns/index.js",
      "types": "./dist/tcp/patterns/index.d.ts"
    },
    "./tcp/interfaces": {
      "import": "./dist/tcp/interfaces/index.js",
      "types": "./dist/tcp/interfaces/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "codes": [
        "dist/codes/index.d.ts"
      ],
      "codes/*": [
        "dist/codes/*"
      ],
      "dtos": [
        "dist/dtos/index.d.ts"
      ],
      "dtos/*": [
        "dist/dtos/*"
      ],
      "exception": [
        "dist/exception/index.d.ts"
      ],
      "exception/*": [
        "dist/exception/*"
      ],
      "messages": [
        "dist/messages/index.d.ts"
      ],
      "messages/*": [
        "dist/messages/*"
      ],
      "tcp": [
        "dist/tcp/index.d.ts"
      ],
      "tcp/*": [
        "dist/tcp/*"
      ],
      "tcp/patterns": [
        "dist/tcp/patterns/index.d.ts"
      ],
      "tcp/patterns/*": [
        "dist/tcp/patterns/*"
      ],
      "tcp/interfaces": [
        "dist/tcp/interfaces/index.d.ts"
      ],
      "tcp/interfaces/*": [
        "dist/tcp/interfaces/*"
      ]
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "@krgeobuk/shared": "workspace:*",
    "@krgeobuk/core": "workspace:*",
    "@nestjs/common": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  },
  "devDependencies": {
    "@krgeobuk/shared": "workspace:*",
    "@krgeobuk/core": "workspace:*",
    "@nestjs/common": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "krgeobuk",
    "account-merge",
    "saga",
    "distributed-transaction",
    "microservices"
  ],
  "author": "krgeobuk",
  "license": "UNLICENSED"
}
