{
  "name": "@krgeobuk/user",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf dist && rm -rf node_modules",
    "verdaccio:login": "pnpm login --registry=http://localhost:4873",
    "publish": "pnpm verdaccio:login && pnpm publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./interfaces": "./dist/interfaces/index.js",
    "./dtos": "./dist/dtos/index.js"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "interfaces": [
        "dist/interfaces/index.d.ts"
      ],
      "interfaces/*": [
        "dist/interfaces/*"
      ],
      "dtos": [
        "dist/dtos/index.d.ts"
      ],
      "dtos/*": [
        "dist/dtos/*"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  },
  "devDependencies": {
    "@krgeobuk/core": "workspace:*",
    "@krgeobuk/swagger": "workspace:*",
    "@krgeobuk/jwt": "workspace:*",
    "@types/node": "^20.19.0",
    "typescript": "^5.8.3"
  }
}
