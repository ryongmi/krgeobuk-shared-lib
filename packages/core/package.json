{
  "name": "@krgeobuk/core",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean && rm -rf dist && rm -rf node_modules",
    "verdaccio:login": "pnpm login --registry=http://localhost:4873",
    "verdaccio:publish": "pnpm publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./decorators": "./dist/decorators/index.js",
    "./dtos": "./dist/dtos/index.js",
    "./entities": "./dist/entities/index.js",
    "./enum": "./dist/enum/index.js",
    "./exception": "./dist/exception/index.js",
    "./filters": "./dist/filters/index.js",
    "./interceptors": "./dist/interceptors/index.js",
    "./interfaces": "./dist/interfaces/index.js",
    "./interfaces/express": "./dist/interfaces/express/index.js",
    "./interfaces/tcp": "./dist/interfaces/tcp/index.js",
    "./logger": "./dist/logger/index.js",
    "./repositories": "./dist/repositories/index.js",
    "./response": "./dist/response/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "decorators": [
        "dist/decorators/index.d.ts"
      ],
      "decorators/*": [
        "dist/decorators/*"
      ],
      "dtos": [
        "dist/dtos/index.d.ts"
      ],
      "dtos/*": [
        "dist/dtos/*"
      ],
      "entities": [
        "dist/entities/index.d.ts"
      ],
      "entities/*": [
        "dist/entities/*"
      ],
      "enum": [
        "dist/enum/index.d.ts"
      ],
      "enum/*": [
        "dist/enum/*"
      ],
      "exception": [
        "dist/exception/index.d.ts"
      ],
      "exception/*": [
        "dist/exception/*"
      ],
      "filters": [
        "dist/filters/index.d.ts"
      ],
      "filters/*": [
        "dist/filters/*"
      ],
      "interceptors": [
        "dist/interceptors/index.d.ts"
      ],
      "interceptors/*": [
        "dist/interceptors/*"
      ],
      "interfaces": [
        "dist/interfaces/index.d.ts"
      ],
      "interfaces/*": [
        "dist/interfaces/*"
      ],
      "interfaces/express": [
        "dist/interfaces/express/index.d.ts"
      ],
      "interfaces/tcp": [
        "dist/interfaces/tcp/index.d.ts"
      ],
      "interfaces/tcp/*": [
        "dist/interfaces/tcp/*"
      ],
      "logger": [
        "dist/logger/index.d.ts"
      ],
      "logger/*": [
        "dist/logger/*"
      ],
      "repositories": [
        "dist/repositories/index.d.ts"
      ],
      "repositories/*": [
        "dist/repositories/*"
      ],
      "response": [
        "dist/response/index.d.ts"
      ],
      "response/*": [
        "dist/response/*"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "utils/*": [
        "dist/utils/*"
      ]
    }
  },
  "sideEffects": false,
  "private": true,
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "lodash-es": "^4.17.21",
    "nest-winston": "^1.10.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "lodash-es": "^4.17.21",
    "nest-winston": "^1.10.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}

