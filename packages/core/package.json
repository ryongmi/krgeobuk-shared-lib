{
  "name": "@krgeobuk/core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dtos": "./dist/dtos/index.js",
    "./validation": "./dist/validation/index.js"
  },
  "typesVersions": {
    "*": {
      "dtos": [
        "dist/dtos/index.d.ts"
      ],
      "dtos/*": [
        "dist/dtos/*"
      ],
      "validation": [
        "dist/validation/index.d.ts"
      ],
      "validation/*": [
        "dist/validation/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean"
  },
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3"
  },
  "devDependencies": {
    "@nestjs/swagger": "^11.2.0",
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
